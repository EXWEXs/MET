\end_layout

\begin_layout Subsubsection*
plot_max = 0.0;
\end_layout

\begin_layout Subsubsection*
colorbar_spacing = 1;
\end_layout

\begin_layout Subsubsection*
};
\end_layout

\begin_layout Subsubsection*
object_plot = {
\end_layout

